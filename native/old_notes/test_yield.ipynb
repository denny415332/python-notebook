    "from time import sleep\n",
    "\n",
    "\n",
    "def test_yield():\n",
    "    for i in range(10):\n",
    "        yield i\n",
    "\n",
    "    print(\"end yield\")\n",
    "\n",
    "\n",
    "for i in test_yield():\n",
    "    print(i + 1, end=\" \")\n",
    "    sleep(1)\n",
    "    print(\"\")\n",
    "\n",
    "print(\"end\")\n"
